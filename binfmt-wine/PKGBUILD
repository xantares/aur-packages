pkgname=binfmt-wine
pkgver=1.1
pkgrel=1
pkgdesc="register wine interpreter"
arch=(any)
url="http://man7.org/linux/man-pages/man8/systemd-binfmt.8.html"
license=(GPL)
depends=('wine')
makedepends=()
source=("wine.conf")
md5sums=('599bc907c9ae3fa371e8032b6db735dc')

package () {
  cd "$srcdir"
  install -Dm 644 wine.conf "$pkgdir/etc/binfmt.d/wine.conf"
}
