pkgname=binfmt-wine
pkgver=1
pkgrel=1
pkgdesc="register wine interpreter"
arch=(any)
url="http://packages.debian.org/en/sid/binfmt-support"
license=(GPL)
depends=('binfmt-support' 'wine')
makedepends=('binfmt-support')
source=("wine.conf")
md5sums=('599bc907c9ae3fa371e8032b6db735dc')
install=$pkgname.install

package () {
  cd "$srcdir"
  install -Dm 644 wine.conf "$pkgdir/etc/binfmt.d/wine.conf"
}
